#include <AmbientObject.s3d>
#include <MeshFactory.s3d>

class EscapePod: AmbientObject {
	
	EscapePod(meshFactory);
	isDestroyable();
	getAmbientType();
};

function EscapePod::EscapePod(meshFactory){
	AmbientObject::this.init( meshFactory.getAmbientMeshes(ESCAPE_POD), ESCAPE_POD );
}

function EscapePod::isDestroyable() {
	return true;
}

function EscapePod::getAmbientType() {
	return ESCAPE_POD;
}
