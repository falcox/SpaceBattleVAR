#include <FighterJet.s3d>
#include <MeshFactory.s3d>

class MillenniumFalcon: FighterJet {
	MillenniumFalcon(meshFactory);
};

function MillenniumFalcon::MillenniumFalcon(meshFactory)
{
	FighterJet::this.init(meshFactory, MILLFALC_TYPE);
	//TODO: Fix camera offset
	FighterJet::this.setCameraOffset({[10,-60], [2.8,-2.8]});
}
