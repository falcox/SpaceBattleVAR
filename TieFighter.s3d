#include <FighterJet.s3d>
#include <MeshFactory.s3d>

class TieFighter: FighterJet {
	TieFighter(meshFactory);
};

function TieFighter::TieFighter(meshFactory)
{
	FighterJet::this.init(meshFactory.getFighterMeshes(TIE_TYPE), TIE_TYPE);
	//TODO: Fix camera offset
	FighterJet::this.setCameraOffset({[10,-60], [2.8,-2.8]});
}
