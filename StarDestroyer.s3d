#include <AmbientObject.s3d>
#include <MeshFactory.s3d>

class StarDestroyer: AmbientObject {
	
	StarDestroyer(meshFactory);
	isDestroyable();
	getAmbientType();
};

function StarDestroyer::StarDestroyer(meshFactory){
	AmbientObject::this.init( meshFactory.getAmbientMeshes(STAR_DESTROYER), STAR_DESTROYER );
	//object.setScale(10);
}

function StarDestroyer::isDestroyable() {
	return false;
}

function StarDestroyer::getAmbientType() {
	return STAR_DESTROYER;
}
