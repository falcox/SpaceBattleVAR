#include <AmbientObject.s3d>
#include <MeshFactory.s3d>

class EmperorShuttle: AmbientObject {
	
	EmperorShuttle(meshFactory);
	isDestroyable();
	getAmbientType();
};

function EmperorShuttle::EmperorShuttle(meshFactory){
	AmbientObject::this.init( meshFactory.getAmbientMeshes(EMPEROR_SHUTTLE), EMPEROR_SHUTTLE );
}

function EmperorShuttle::isDestroyable() {
	return true;
}

function EmperorShuttle::getAmbientType() {
	return EMPEROR_SHUTTLE;
}
